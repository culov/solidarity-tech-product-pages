@media (max-width: 768px) { /* Adjust based on your mobile breakpoint */
  #tocButton{
    position: fixed; 
    bottom: 10px; 
    right: 10px; 
    z-index: 100
  }
  #tocContainer {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 50%; /* Take up half the screen */
    overflow-y: auto; /* Make it scrollable */
    background-color: white; /* Background color */
    z-index: 99; /* Ensure it's above other content, adjust as needed */
    border-top: 1px solid #ccc; /* Optional border */
  }
  .toc.toc-sidebar{
    display: none;
  }
  .toc { /* Prevent the TOC from being sticky on mobile */
    position: static;
  }
}

@media (min-width: 768px) { 
  #tocContainer, #tocButton {
    display: none;
  }
    .toc{
      position: sticky;
      top: 80px;
      
    .active-toc-item {
        border-left: 3px solid $color-red;
        padding-left: 5px;
        display: block;
      }
    }
}
