  .navbar-toggle#nav-toggle{
    cursor: pointer; 
    padding: 10px 35px 16px 0px; 
    margin-top: 20px;
    margin-right: 20px;

    span, span:before, span:after {
      cursor: pointer;
      border-radius: 1px;
      height: 3px;
      width: 27px;
      background: black;
      position: absolute;
      display: block;
      content: '';
    }

    span:before {
      top: -9px; 
    }
    span:after {
      bottom: -9px;
    }

    span, span:before, span:after {
      transition: all 300ms ease-in-out;
    }
    &.active span {
      background-color: transparent;
    }
    &.active span:before, &.active span:after {
      top: 0;
    }
    &.active span:before {
      transform: rotate(45deg);
    }
    &.active span:after {
      transform: rotate(-45deg);
    }


  }


